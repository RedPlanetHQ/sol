    "simple-oauth2": "^5.0.0",
    "socket.io": "^4.7.5",
    "supertokens-node": "^21.0.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.20"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.1",
    "@sigma/eslint-config": "workspace:*",
    "@sigma/typescript-config": "workspace:*",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.3.105",
    "@trigger.dev/build": "3.3.13",
