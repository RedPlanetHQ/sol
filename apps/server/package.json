{
  "name": "server",
  "version": "0.3.6",
  "description": "Sigma server",
  "author": "Sigma",
  "private": true,
  "keywords": [
    "nestjs",
    "prisma",
    "prisma client",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tegonhq/sigma.git"
  },
  "bugs": {
    "url": "https://github.com/tegonhq/sigma.git/issues"
  },
  "scripts": {
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start-prod": "pnpm run migrate:deploy && pnpm run generate && node dist/main",
    "start-prod-with-prisma": "pnpm run start-prod && pnpm prisma:studio",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "prisma migrate deploy --preview-feature",
    "migrate:status": "prisma migrate status --preview-feature",
    "migrate:resolve": "prisma migrate resolve --preview-feature",
    "prisma:studio": "prisma studio",
    "generate": "prisma generate",
    "db:init-resources": "pnpm run migrate && pnpm run generate",
    "trigger-deploy": "npx trigger.dev@latest deploy --self-hosted",
    "trigger-dev": "npx trigger.dev@latest dev"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.6",
    "@ai-sdk/openai": "^1.1.9",
    "@devoxa/prisma-relay-cursor-connection": "3.1.1",
    "@google-cloud/storage": "^7.10.0",
    "@hocuspocus/extension-database": "^2.13.7",
    "@hocuspocus/server": "^2.13.7",
    "@hocuspocus/transformer": "^2.13.7",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.1.0",
    "@nestjs/bullmq": "^10.2.3",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "10.3.1",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.1",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/platform-socket.io": "^10.3.1",
    "@nestjs/platform-ws": "^10.4.5",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/websockets": "^10.3.1",
    "@octokit/rest": "^21.0.2",
    "@paciolan/remote-module-loader": "^3.0.3",
    "@prisma/client": "^5.21.0",
    "@prisma/instrumentation": "^5.21.0",
    "@qdrant/js-client-rest": "^1.12.0",
    "@sigma/editor-extensions": "workspace:*",
    "@sigma/types": "workspace:*",
    "@tiptap/html": "^2.10.3",
    "@trigger.dev/sdk": "3.3.13",
    "@types/multer": "^1.4.11",
    "@types/turndown": "^5.0.5",
    "@xenova/transformers": "^2.16.1",
    "axios": "^1.6.7",
    "bcrypt": "5.1.1",
    "bullmq": "^5.34.0",
    "cache-manager": "^5.7.6",
    "chalk": "^4.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie": "^0.6.0",
    "cron": "^3.1.7",
    "cron-parser": "^4.9.0",
    "date-fns": "^3.6.0",
    "execa": "^9.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "knex": "^3.1.0",
    "marked": "^14.1.2",
    "nest-winston": "^1.9.7",
    "nestjs-prisma": "0.22.0",
    "nodemailer": "^6.9.13",
